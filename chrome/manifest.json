{
  "manifest_version": 3,
  "name": "TT Shop GMV Helper",
  "version": "1.0",
  "description": "帮助TikTok Shop卖家快速查看和复制GMV数据",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "../shared/assets/images/icon16.png",
      "48": "../shared/assets/images/icon48.png",
      "128": "../shared/assets/images/icon128.png"
    }
  },
  "icons": {
    "16": "../shared/assets/images/icon16.png",
    "48": "../shared/assets/images/icon48.png",
    "128": "../shared/assets/images/icon128.png"
  }
} 